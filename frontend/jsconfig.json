{
	"exclude": ["node_modules", "dist", "static"],
	"compilerOptions": {
		"target": "es2020",
		"allowSyntheticDefaultImports": false,
		"baseUrl": "./",
		"paths": {
			"Components/*": ["src/Components/*"],
			"Pages/*": ["src/Pages/*"],
			"Theme/*": ["src/Theme/*"],
			"Utils/*": ["src/Utils/*"],
			"Root/*": ["src/*"]
		}
	}
}
